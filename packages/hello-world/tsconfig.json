{
  "compilerOptions": {
      "strict": true,
      "strictPropertyInitialization": false,
      "declaration": true,
      "declarationMap": true,
      "experimentalDecorators": true,
      "noUnusedLocals": true,
      "emitDecoratorMetadata": true,
      "downlevelIteration": true,
      "jsx": "react",
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es5",
      "lib": [
          "es6",
          "dom"
      ],
      "skipLibCheck": true,
      "sourceMap": true,
      "rootDir": "src",
      "outDir": "lib"
  },
  "include": [
      "src"
  ]
}
